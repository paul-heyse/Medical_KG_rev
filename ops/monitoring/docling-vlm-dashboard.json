{
    "dashboard": {
        "id": null,
        "title": "Docling VLM Service Dashboard",
        "tags": [
            "docling",
            "vlm",
            "pdf-processing",
            "doctags"
        ],
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "VLM Service Health",
                "type": "stat",
                "targets": [
                    {
                        "expr": "docling_vlm_service_health",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "green",
                                    "value": 1
                                }
                            ]
                        },
                        "mappings": [
                            {
                                "type": "value",
                                "options": {
                                    "0": {
                                        "text": "Unhealthy"
                                    },
                                    "1": {
                                        "text": "Healthy"
                                    }
                                }
                            }
                        ]
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "PDF Processing Rate",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "rate(docling_vlm_pdf_processed_total[5m])",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "PDF Processing Duration",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.50, rate(docling_vlm_pdf_processing_duration_seconds_bucket[5m]))",
                        "legendFormat": "50th percentile",
                        "refId": "A"
                    },
                    {
                        "expr": "histogram_quantile(0.95, rate(docling_vlm_pdf_processing_duration_seconds_bucket[5m]))",
                        "legendFormat": "95th percentile",
                        "refId": "B"
                    },
                    {
                        "expr": "histogram_quantile(0.99, rate(docling_vlm_pdf_processing_duration_seconds_bucket[5m]))",
                        "legendFormat": "99th percentile",
                        "refId": "C"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 30
                                },
                                {
                                    "color": "red",
                                    "value": 60
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "VLM Model Performance",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "docling_vlm_model_inference_time_seconds",
                        "legendFormat": "{{model_name}} - {{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 5,
                "title": "Doctags Generation Metrics",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "docling_vlm_doctags_generated_total",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    },
                    {
                        "expr": "docling_vlm_text_blocks_extracted_total",
                        "legendFormat": "Text Blocks - {{instance}}",
                        "refId": "B"
                    },
                    {
                        "expr": "docling_vlm_tables_extracted_total",
                        "legendFormat": "Tables - {{instance}}",
                        "refId": "C"
                    },
                    {
                        "expr": "docling_vlm_figures_extracted_total",
                        "legendFormat": "Figures - {{instance}}",
                        "refId": "D"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "GPU Memory Usage for VLM",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "docling_vlm_gpu_memory_usage_bytes",
                        "legendFormat": "{{instance}} - GPU {{device_id}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "bytes",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 8000000000
                                },
                                {
                                    "color": "red",
                                    "value": 12000000000
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 24,
                    "y": 8
                }
            },
            {
                "id": 7,
                "title": "VLM Processing Errors",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "rate(docling_vlm_processing_errors_total[5m])",
                        "legendFormat": "{{error_type}} - {{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 8,
                "title": "Batch Processing Queue",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "docling_vlm_batch_queue_size",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 10
                                },
                                {
                                    "color": "red",
                                    "value": 50
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 16
                }
            },
            {
                "id": 9,
                "title": "VLM Model Warm-up Status",
                "type": "stat",
                "targets": [
                    {
                        "expr": "docling_vlm_model_warmup_status",
                        "legendFormat": "{{model_name}} - {{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "green",
                                    "value": 1
                                }
                            ]
                        },
                        "mappings": [
                            {
                                "type": "value",
                                "options": {
                                    "0": {
                                        "text": "Not Ready"
                                    },
                                    "1": {
                                        "text": "Ready"
                                    }
                                }
                            }
                        ]
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 24,
                    "y": 16
                }
            },
            {
                "id": 10,
                "title": "Doctags Quality Metrics",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "docling_vlm_doctags_quality_score",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "min": 0,
                        "max": 1,
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "red",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.7
                                },
                                {
                                    "color": "green",
                                    "value": 0.9
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 24
                }
            },
            {
                "id": 11,
                "title": "VLM Service Statistics",
                "type": "table",
                "targets": [
                    {
                        "expr": "docling_vlm_service_stats",
                        "format": "table",
                        "instant": true,
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "align": "auto",
                            "displayMode": "auto"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 24
                }
            },
            {
                "id": 12,
                "title": "PDF Processing Throughput",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "rate(docling_vlm_pdf_processed_total[1m]) * 60",
                        "legendFormat": "{{instance}}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 1,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": {
                                "mode": "none",
                                "group": "A"
                            },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": {
                                "type": "linear"
                            },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": {
                                "mode": "off"
                            }
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 24,
                    "y": 24
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "30s",
        "annotations": {
            "list": [
                {
                    "name": "VLM Service Deployments",
                    "datasource": "Prometheus",
                    "expr": "changes(docling_vlm_service_health[1m])",
                    "iconColor": "blue",
                    "enable": true,
                    "step": "1m"
                },
                {
                    "name": "Model Updates",
                    "datasource": "Prometheus",
                    "expr": "changes(docling_vlm_model_warmup_status[1m])",
                    "iconColor": "orange",
                    "enable": true,
                    "step": "1m"
                }
            ]
        },
        "templating": {
            "list": [
                {
                    "name": "instance",
                    "type": "query",
                    "query": "label_values(docling_vlm_pdf_processed_total, instance)",
                    "refresh": 1,
                    "includeAll": true,
                    "multi": true
                },
                {
                    "name": "model_name",
                    "type": "query",
                    "query": "label_values(docling_vlm_model_inference_time_seconds, model_name)",
                    "refresh": 1,
                    "includeAll": true,
                    "multi": true
                }
            ]
        },
        "links": [
            {
                "title": "Docling VLM Documentation",
                "url": "https://docs.medical-kg.com/services/docling-vlm"
            },
            {
                "title": "Doctags Format Specification",
                "url": "https://docs.medical-kg.com/formats/doctags"
            },
            {
                "title": "VLM Performance Tuning Guide",
                "url": "https://docs.medical-kg.com/guides/vlm-performance"
            }
        ]
    }
}
