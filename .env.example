# Core application settings
MK_ENV=dev
MK_SERVICE_NAME=medical-kg
MK_TELEMETRY__EXPORTER=console
MK_TELEMETRY__ENDPOINT=
MK_TELEMETRY__SAMPLE_RATIO=0.1
MK_OBSERVABILITY__LOGGING__LEVEL=INFO
MK_OBSERVABILITY__LOGGING__CORRELATION_ID_HEADER=X-Correlation-ID
MK_OBSERVABILITY__LOGGING__SCRUB_FIELDS__0=password
MK_OBSERVABILITY__LOGGING__SCRUB_FIELDS__1=token
MK_OBSERVABILITY__METRICS__PATH=/metrics
MK_OBSERVABILITY__SENTRY__DSN=
MK_OBSERVABILITY__SENTRY__TRACES_SAMPLE_RATE=0.1
MK_OBSERVABILITY__SENTRY__SEND_DEFAULT_PII=false

# Vault configuration
MK_VAULT__ENABLED=false
MK_VAULT__ADDRESS=
MK_VAULT__TOKEN=
MK_VAULT__NAMESPACE=

# Feature flags
MK_FEATURE_FLAGS__FLAGS__ADAPTERS_DYNAMIC=true
MK_FEATURE_FLAGS__FLAGS__ENABLE_TRACING=true

# MinerU GPU processing
MK_MINERU__ENABLED=false
MK_MINERU__CLI_COMMAND=mineru
MK_MINERU__EXPECTED_VERSION=>=2.5.4
MK_MINERU__CUDA_VERSION=12.8
MK_MINERU__WORKERS__COUNT=4
MK_MINERU__WORKERS__VRAM_PER_WORKER_GB=7
MK_MINERU__WORKERS__TIMEOUT_SECONDS=300
MK_MINERU__WORKERS__BATCH_SIZE=4
MK_MINERU__CPU__ENABLED=true
MK_MINERU__CPU__OMP_NUM_THREADS=4
MK_MINERU__CPU__MKL_NUM_THREADS=4
