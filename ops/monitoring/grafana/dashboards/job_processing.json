{
  "title": "Job Processing",
  "uid": "job-processing",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "graph",
      "title": "Job Duration Histogram",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(job_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "{{operation}}"
        }
      ]
    },
    {
      "type": "table",
      "title": "Business Events",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "business_events_total",
          "format": "table"
        }
      ],
      "transformations": [
        { "id": "labelsToFields" }
      ]
    }
  ]
}
